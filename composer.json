{
  "name": "fabithub/inertia-ci4",
  "description": "The Codeigniter 4 adapter for inertia.js",
  "version": "0.0.1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Krishna Gujjjar",
      "email": "krishnagujjjar@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Inertia\\": "src/"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "codeigniter4/framework": "^4.3|^4.4"
  },
  "require-dev": {
    "codeigniter4/devkit": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
